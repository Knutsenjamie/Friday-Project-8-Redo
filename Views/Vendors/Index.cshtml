@using PierresOrderForm.Models;

<h1>Vendors</h1>

@if (@Model.Count == 0)
{
  <h3>No Vendors have been added yet!</h3>
} 

@foreach (Vendors vendors in Model)
{
  <h3><a href='/vendors/@vendors.Id'>@vendors.VendorsName</a></h3>
  <h3><a href='/vendors/@vendors.Id'>@vendors.VendorDescription</a></h3>
}

<p><a href='/'>Back home</a></p>